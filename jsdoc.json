{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./resources/js/components"],
        "includePattern": "\\.(ts|vue)$"
    },
    "opts": {
        "template": "node_modules/better-docs",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "plugins": [
        "node_modules/jsdoc-vuejs",
        "node_modules/better-docs/typescript"
    ]
}
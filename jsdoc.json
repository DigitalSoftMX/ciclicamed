{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./resources/js/components"],
        "includePattern": "\\.(ts)$"
    },
    "opts": {
        "template": "node_modules/better-docs",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "plugins": [
        "node_modules/better-docs/typescript"
    ],
    "templates": {
        "better-docs": {
            "name": "Ciclicamed"
        }
    }
}